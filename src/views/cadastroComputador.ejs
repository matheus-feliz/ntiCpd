<%-include('./include/head')%>
    <link rel="stylesheet" type="text/css" href="./css/styleCadastroPc.css">


    <section>
        <%-include('./include/navBar')%>
            <%-include('include/messages');%>
                <h1 class="text-center "> Cadastro do computador
                </h1>
                <div class="grid-form">
                    <form action="/registro" method="POST">
                        <fieldset class="form-contact">
                            <div class="form-group">
                                <label class="label-form" for="telefone">Telefone</label>
                                <input class="input-form" id="telefone" name="telefone" type="tel" placeholder="(77) 3333-3333">
                            </div>
                            <div class="form-group">
                                <label class="label-form" for="unidade">Unidade</label>
                                <input class="input-form" id="unidade" name="unidade" type="text" placeholder="nome da unidade">
                            </div>
                            <div class="form-group">
                                <label class="label-form" for="responsavel">Responsável</label>
                                <input class="input-form" id="responsavel" name="responsavel" type="text" placeholder="nome do Responsável">
                            </div>
                            <div class="form-group">
                                <label class="label-form" for="tombo">Tombo</label>
                                <input class="input-form" id="tombo" name="tombo" type="text" placeholder="13545357">
                            </div>
                            <div class="form-group">
                                <label class="label-form" for="tipo">Tipo do equipamento</label>
                                <input class="input-form" id="tipo" name="tipo" type="text" placeholder="cpu">
                            </div>

                            <div class="form-espaco">

                            </div>
                            <div class="form-group">
                                <label class="label-form" for="observacao">Observação</label>
                                <textarea class="input-form" id="observacao" name="observacao" cols="30" rows="10"> </textarea>
                            </div>
                            <button class="button" type="submit"><img class="button-img"  src="./include/img/salvar.svg"></button>
                        </fieldset>
                    </form>

                </div>
                <%-include('./include/barraLateral')%>
    </section>

    <%-include('./include/footer')%>